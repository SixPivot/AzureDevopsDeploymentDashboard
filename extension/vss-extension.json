{
  "manifestVersion": 1,
  "id": "hack2022-ado-hw",
  "publisher": "hack2022-ado",
  "version": "1.0.14",
  "name": "Release Dashboard",
  "description": "A dashboard to view a summary of all deployments",
  "demands": [
    "api-version/3.0"
  ],
  "categories": [
    "Azure Repos"
  ],
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "icons": {
    "default": "img/sixpivot.png"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "scopes": [
    "vso.build"
  ],
  "contributions": [
    {
      "id": "deployment-dashboard",
      "type": "ms.vss-web.hub",
      "description": "The best darn deployment dashboard you're going to see",
      "targets": [
        "ms.vss-build-web.build-release-hub-group"
      ],
      "properties": {
        "name": "Deployment Dashboard",
        "uri": "dist/dashboard/dashboard.html",
        "order": 1
      }
    }
  ]
}
